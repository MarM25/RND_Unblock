          "*://*.op-marburg.de/*",
          "*://*.ostsee-zeitung.de/*",
          "*://*.paz-online.de/*",
          "*://*.radio.net/*",
          "*://*.reisereporter.de/*",
          "*://*.remszeitung.de/*",
          "*://*.rga.de/*",
          "*://*.sn-online.de/*",
          "*://*.sportbuzzer.de/*",
          "*://*.solinger-tageblatt.de/*",
